{
  "name": "typescript-base",
  "version": "0.1.0",
  "description": "Example base for typescript projects",
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "build": "tsc",
    "console": "node console.js",
    "cover": "NODE_ENV=testing jest --coverage --runInBand --forceExit",
    "test": "NODE_ENV=testing jest --runInBand --forceExit",
    "test-inspect": "NODE_ENV=testing node --inspect --debug-brk jest",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "eslint-check": "eslint --print-config .eslintrc.js --ignore-pattern ./.eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint \"**/*.js\" --ignore-pattern ./.eslintrc.js",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.js$ | xargs eslint",
    "lint-fix": "eslint \"**/*.js\" --fix --ignore-pattern ./.eslintrc.js",
    "precommit": "npm run lint-diff",
    "postinstall": "npm run build",
    "outdated": "npm outdated --depth 0",
    "pretest": "npm run lint",
    "prestart": "npm run lint",
    "migrations": "tsc && sequelize db:migrate",
    "migrations-test": "tsc && NODE_ENV=testing sequelize db:migrate",
    "start": "ts-node-dev --respawn --transpileOnly ./server.ts"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "app.js",
  "author": "Wolox",
  "homepage": "https://github.com/mpicollo-wolox/typescript-base",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpicollo-wolox/typescript-base.git"
  },
  "bugs": {
    "url": "https://github.com/mpicollo-wolox/typescript-base/issues",
    "email": "tls@wolox.com.ar"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/console.js",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/migrations/**",
      "!**/config/**",
      "!**/scripts/**",
      "!**/docs/**"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.spec.ts?(x)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/app.ts"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-wolox-logger": "0.0.2",
    "jwt-simple": "^0.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.4.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rollbar": "^2.3.9",
    "sequelize": "^5.15.0",
    "umzug": "^2.1.0",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {
    "@types/babel-core": "^6.25.6",
    "@types/babel-traverse": "^6.25.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.18",
    "@types/jwt-simple": "^0.5.33",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.6.2",
    "@types/pg": "^7.4.14",
    "@types/request-promise": "^4.1.44",
    "@types/sequelize": "^4.28.3",
    "@types/supertest": "^2.0.8",
    "@types/umzug": "^2.2.2",
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^25.0.0",
    "babel-preset-es2015": "6.24.1",
    "coveralls": "^3.0.0",
    "dictum.js": "^1.0.0",
    "dotenv": "^5.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.3.1",
    "eslint-config-wolox": "^2.2.1",
    "eslint-config-wolox-node": "^1.0.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^0.14.3",
    "istanbul": "^0.4.3",
    "jest": "^24.9.0",
    "mocha": "^5.0.1",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "prompt": "^1.0.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "1.0.0-pre.40",
    "typescript": "^3.5.2"
  }
}
